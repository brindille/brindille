{
  "name": "no-framework-boilerplate",
  "version": "0.0.0",
  "description": "No js framework, gulp + browserify based boilerplate",
  "main": "app/app.js",
  "scripts": {
    "install": "napa",
    "start": "npm run server",
    "dev": "gulp build --watch",
    "server": "gulp server --watch",
    "server-proxy": "gulp server --watch --proxy localhost:3333",
    "prod": "gulp build --production"
  },
  "keywords": [
    "boilerplate",
    "javascript",
    "gulp",
    "browserify"
  ],
  "author": "vdaguenet",
  "license": "MIT",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "PxLoader": "./node_modules/PxLoader/dist/pxloader-all.js"
  },
  "browserify-shim": {
    "PxLoader": "PxLoader"
  },
  "napa": {
    "PxLoader": "superguigui/PxLoader"
  },
  "devDependencies": {
    "aliasify": "^1.5.1",
    "envify": "^3.2.0",
    "stringify": "^3.1.0",
    "stripify": "^2.0.0",
    "rimraf": "^2.2.8",
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "del": "^1.1.1",
    "defaults": "^1.0.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-minify-css": "^0.4.5",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-filter": "^2.0.2",
    "gulp-plumber": "^0.6.6",
    "gulp-stylus": "^2.0.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.3",
    "napa": "^1.2.0",
    "pretty-hrtime": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0",
    "minimist": "^1.1.0",
    "connect-history-api-fallback": "0.0.5",
    "browser-sync": "^2.2.1"
  },
  "dependencies": {
    "gsap": "^1.15.1",
    "bindall-standalone": "^1.0.5",
    "debounce": "^1.0.0",
    "domready": "^1.0.7",
    "dot": "^1.0.3",
    "inherits": "^2.0.1",
    "mobile-detect": "^0.4.3",
    "page": "^1.6.0",
    "promise": "^6.1.0",
    "stylus-media-queries": "^1.0.0",
    "stylus-easing": "^1.0.0",
    "for-each": "^0.3.2",
    "just-next-tick": "0.0.0",
    "emitter-component": "^1.1.1",
    "dom-walk": "^0.1.1",
    "domify": "^1.3.1",
    "watchjs": "0.0.0"
  }
}
