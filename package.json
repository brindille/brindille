{
  "name": "no-framework-boilerplate",
  "version": "0.0.0",
  "description": "No js framework, gulp + browserify based boilerplate",
  "main": "app/app.js",
  "scripts": {
    "install": "napa"
  },
  "keywords": [
    "boilerplate",
    "javascript",
    "gulp",
    "browserify"
  ],
  "author": "vdaguenet",
  "license": "MIT",
  "browserify": {
    "transform": [
      "browserify-shim",
      "html-browserify"
    ]
  },
  "browser": {
    "fit": "./node_modules/fit/fit.min.js",
    "forEach": "./node_modules/foreach.js/dist/foreach.min.js",
    "PxLoader": "./node_modules/PxLoader/dist/pxloader-all.js",
    "TweenMax": "./node_modules/greensock/src/minified/TweenMax.min.js"
  },
  "browserify-shim": {
    "fit": "fit",
    "forEach": "forEach",
    "PxLoader": "PxLoader",
    "TweenMax": "TweenMax"
  },
  "napa": {
    "fit": "soulwire/fit.js",
    "foreach.js": "toddmotto/foreach",
    "greensock": "greensock/GreenSock-JS",
    "PxLoader": "thinkpixellab/PxLoader"
  },
  "devDependencies": {
    "aliasify": "^1.5.1",
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-jshint": "^1.9.2",
    "gulp-minify-css": "^0.4.5",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.3.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.3",
    "html-browserify": "0.0.4",
    "jshint-stylish": "^1.0.0",
    "napa": "^1.2.0",
    "pretty-hrtime": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0",
    "yargs": "^2.3.0"
  },
  "dependencies": {
    "extend": "^2.0.0",
    "page": "^1.6.0"
  }
}
