{
  "name": "brindille",
  "version": "1.0.0",
  "description": "Web project boilerplate, stylus + webpack + babel + symfony + twig",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack & open http://localhost:8080/",
    "build": "webpack --env.prod",
    "svg": "svg-sprite -w 1024 -h 1024 --svg-xmldecl false --svg-doctype false -s --symbol-dest ./src/views --symbol-dimensions true --ss icons.svg --symbol-bust false public/assets/images/icons/*.svg"
  },
  "aliasify": {
    "aliases": {
      "lib": "./src/lib/",
      "config": "./src/config/",
      "views": "./src/views/",
      "data": "./data/"
    }
  },
  "author": "Hellohikimori",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer-stylus": "^0.14.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^1.0.0",
    "husky": "^1.0.0-rc.15",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "prettier": "1.14.3",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite": "*",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-livereload-plugin": "^2.1.1",
    "webpackbar": "^2.6.3",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "brindille-component": "*",
    "brindille-interactive-component": "*",
    "brindille-preloader": "*",
    "brindille-resize": "*",
    "brindille-router": "0.0.4",
    "brindille-scroll": "*",
    "brindille-stylus-extends": "*",
    "dom-classes": "*",
    "dush": "^3.0.5",
    "gsap": "*",
    "stylus-easing": "*",
    "stylus-media-queries": "*",
    "stylus-reset": "*",
    "whatwg-fetch": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brindille/brindille.git"
  },
  "homepage": "https://github.com/brindille/brindille",
  "bugs": {
    "url": "https://github.com/brindille/brindille/issues"
  }
}
